{
  "name": "chalkee",
  "version": "1.4.0",
  "description": "Advanced terminal styling with plugin architecture, tree-shaking, and template literal chaining",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "terminal",
    "color",
    "ansi",
    "chalk",
    "styling",
    "typescript",
    "tree-shaking",
    "console",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ansi-styles": "6.2.1",
    "chalk": "^4.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0"
  },
  "dependencies": {
    "log-update": "^7.0.2"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "build:min": "vite build --minify && tsc --emitDeclarationOnly",
    "test": "tsx --test src/comprehensive.test.ts",
    "test:watch": "tsx --test --watch src/comprehensive.test.ts",
    "test:no-color": "tsx --test src/no-color.test.ts"
  }
}